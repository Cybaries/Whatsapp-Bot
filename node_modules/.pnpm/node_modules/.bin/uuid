#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/9a906770-254e-4190-995e-b9cc5919e43a/Projects/Whatsapp-Bot/node_modules/.pnpm/uuid@9.0.1/node_modules/uuid/dist/bin/node_modules:/mnt/9a906770-254e-4190-995e-b9cc5919e43a/Projects/Whatsapp-Bot/node_modules/.pnpm/uuid@9.0.1/node_modules/uuid/dist/node_modules:/mnt/9a906770-254e-4190-995e-b9cc5919e43a/Projects/Whatsapp-Bot/node_modules/.pnpm/uuid@9.0.1/node_modules/uuid/node_modules:/mnt/9a906770-254e-4190-995e-b9cc5919e43a/Projects/Whatsapp-Bot/node_modules/.pnpm/uuid@9.0.1/node_modules:/mnt/9a906770-254e-4190-995e-b9cc5919e43a/Projects/Whatsapp-Bot/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/9a906770-254e-4190-995e-b9cc5919e43a/Projects/Whatsapp-Bot/node_modules/.pnpm/uuid@9.0.1/node_modules/uuid/dist/bin/node_modules:/mnt/9a906770-254e-4190-995e-b9cc5919e43a/Projects/Whatsapp-Bot/node_modules/.pnpm/uuid@9.0.1/node_modules/uuid/dist/node_modules:/mnt/9a906770-254e-4190-995e-b9cc5919e43a/Projects/Whatsapp-Bot/node_modules/.pnpm/uuid@9.0.1/node_modules/uuid/node_modules:/mnt/9a906770-254e-4190-995e-b9cc5919e43a/Projects/Whatsapp-Bot/node_modules/.pnpm/uuid@9.0.1/node_modules:/mnt/9a906770-254e-4190-995e-b9cc5919e43a/Projects/Whatsapp-Bot/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../uuid/dist/bin/uuid" "$@"
else
  exec node  "$basedir/../uuid/dist/bin/uuid" "$@"
fi
